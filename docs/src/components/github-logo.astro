---
import githubDark from "/github--dark.svg";
import githubLight from "/github--light.svg"
const baseURL = import.meta.env.BASE_URL;
---

<img class="hidden dark:block h-8" src={baseURL+githubDark} alt="GitHub Dark" />
<img class="block dark:hidden h-8" src={baseURL+githubLight} alt="GitHub Light" />
